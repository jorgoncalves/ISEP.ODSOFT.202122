      <forcePush>true</forcePush>
      <tagsToPush>
        <hudson.plugins.git.GitPublisher_-TagToPush>
          <targetRepoName>origin</targetRepoName>
          <tagName>Build_$BUILD_NUMBER-$CUSTOM_STATUS</tagName>
          <tagMessage>The build $BUILD_NUMBER as concluded with status - $CUSTOM_STATUS.

$BUILD_TAG</tagMessage>
